@prefix pro: <http://purl.org/hpi/patchr#> .
@prefix pr: <http://purl.org/ontology/prv/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix ftr: <https://w3id.org/ftr#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix vivo: <http://vivoweb.org/ontology/core#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix : <http://www.example.org/me#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .

:testResultSetShape
  a sh:NodeShape ;
  sh:targetClass ftr:TestResultSet ;
  sh:property [
    sh:predicate dcterms:identifier ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    sh:predicate dcterms:title ;
    sh:or ( [ sh:datatype xsd:string ] [ sh:datatype rdf:langString ] ) ;
  ] ;
  sh:property [
    sh:predicate dcterms:license ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    sh:predicate prov:wasGeneratedby ;
    sh:node :testExecutionActivity ;
  ] ;
  sh:property [
    sh:predicate prov:wasDerivedFrom ;
    sh:node :entityShape ;
  ] ;
  sh:property [
    sh:predicate prov:hadMember ;
    sh:node :testResultShape ;
  ] .

:testShape
  a sh:NodeShape ;
  sh:targetClass ftr:Test, dcat:DataService ;
  sh:property [ sh:predicate a ; sh:hasValue ftr:Test, dcat:DataService ] .

:entityShape
  a sh:NodeShape ;
  sh:targetClass prov:Entity .

:testResultShape
  a sh:NodeShape ;
  sh:targetClass ftr:TestResult ;
  sh:property [
    sh:predicate dcterms:identifier ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    sh:predicate dcterms:title ;
    sh:or ( [ sh:datatype xsd:string ] [ sh:datatype rdf:langString ] ) ;
  ] ;
  sh:property [
    sh:predicate dcterms:description ;
    sh:or ( [ sh:datatype xsd:string ] [ sh:datatype rdf:langString ] ) ;
  ] ;
  sh:property [
    sh:predicate dcterms:license ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:property [
    sh:predicate ftr:status ;
    sh:or ( [ sh:datatype xsd:string ] [ sh:datatype rdf:langString ] ) ;
    sh:minCount 0 ;
  ] ;
  sh:property [
    sh:predicate ftr:log ;
    sh:or ( [ sh:datatype xsd:string ] [ sh:datatype rdf:langString ] ) ;
    sh:minCount 0 ;
  ] ;
  sh:property [
    sh:predicate ftr:completion ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:int ;
    sh:minCount 0 ;
  ] ;
  sh:property [
    sh:predicate ftr:affectedElements ;
    sh:nodeKind sh:IRI ;
    sh:minCount 0 ;
  ] ;
  sh:property [
    sh:predicate ftr:outputFromTest ;
    sh:node :testShape ;
  ] ;
  sh:property [
    sh:predicate prov:wasDerivedFrom ;
    sh:node :entityShape ;
  ] .

:testExecutionActivity
  a sh:NodeShape ;
  sh:targetClass ftr:TestExecutionActivity ;
  sh:property [
    sh:predicate prov:generated ;
    sh:node :testResultShape ;
  ] ;
  sh:property [
    sh:predicate prov:used ;
    sh:node :entityShape ;
  ] ;
  sh:property [
    sh:predicate prov:wasAssociatedWith ;
    sh:node :testShape ;
  ] .
